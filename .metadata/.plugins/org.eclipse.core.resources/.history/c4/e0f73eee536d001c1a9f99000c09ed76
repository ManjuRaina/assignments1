package com.tech.arraylistcollection;

import java.util.Comparator;

public class Emp{
int id;
String name;
int sal;
public Emp(int id, String name) {
	super();
	this.id = id;
	this.name = name;
	
}
@Override
public String toString() {
	return "Emp [id=" + id + ", " + (name != null ? "name=" + name + ", " : "") + "sal=" + sal + "]";
}



    
	
}
