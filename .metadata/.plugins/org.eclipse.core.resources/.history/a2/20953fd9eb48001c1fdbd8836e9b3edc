package comp.technoelevate.inheritance;

public class A 
{
	int x;
	static int y;
	static int m1(int a,int b)
	{
		int res1=a+b;
		return res1;
	}
	static int m2(int a,int b,int c)
	{
		int res2=a+b+c;
		return res2;
	}
	int m3(int d)
	{
		int res3=d;
		return res3;
	}
	int m4(int e)
	{
		int res4=e;
		return res4;
	}


}
